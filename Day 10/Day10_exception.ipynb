   "source": [
    "# Zero Division Error Exception\n",
    "x = input(\"Enter number 1: \")\n",
    "y = input(\"Enter number 2: \")\n",
    "try:\n",
    "    z = int(x) / int(y)\n",
    "except ZeroDivisionError as e:\n",
    "    print(\"Exception Occured: \",e)\n",
    "    z = None\n",
    "print(\"Division is : \",z)"
   ]
  }
